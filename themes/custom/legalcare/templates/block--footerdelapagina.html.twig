              <h2><img src="{{logoFooter}}" /></h2>
              <p>{{content.field_texto_footer |render|striptags|trim}}</p>
              <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-2">
                <li class="ftco-animate"><span><a href="{{content.field_url_red_social_1 |render|striptags|trim}}"><i class="{{content.field_icono_red_social_1[0]['#icons'][0]['#style']}} {{content.field_icono_red_social_1[0]['#icons'][0]['#name']}}"></i></a></span></li>
