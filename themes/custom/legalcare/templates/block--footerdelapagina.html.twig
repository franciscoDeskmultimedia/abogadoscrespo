                  {% if content.field_correo |render|striptags|trim %}
	                  <li><a href="mailto:{{content.field_correo |render|striptags|trim}}"><span class="icon icon-envelope"></span><span class="text">{{content.field_correo |render|striptags|trim}}</span></a></li>
                  {% endif %}
	              </ul>
	            </div>
            </div>
          </div>
          <div class="col-md">
            <div class="ftco-footer-widget mb-4">
